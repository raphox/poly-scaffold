export type T<%= resourceNames.capitalize %> = {
  id: number;
  <%_ attributes.forEach(function({ name, type }) { _%>
    <%= name %>: <%= type %>
  <%_ }) _%>
};

export type T<%= resourceNames.capitalize %>Form = Omit<T<%= resourceNames.capitalize %>, "id">;
export type T<%= resourceNames.capitalize %>FormErrors = Partial<Record<keyof T<%= resourceNames.capitalize %>Form, string>>;
